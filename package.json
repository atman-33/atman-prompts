{
  "name": "atman-prompts",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool to generate prompt files in a structured directory format",
  "author": "atman-33",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "prepare": "husky"
  },
  "keywords": [
    "cli",
    "prompts",
    "generator",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atman-33/atman-prompts.git"
  },
  "homepage": "https://github.com/atman-33/atman-prompts#readme",
  "bin": {
    "atman-prompts": "dist/index.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "templates/",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.9",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "commander": "^13.0.0"
  }
}
